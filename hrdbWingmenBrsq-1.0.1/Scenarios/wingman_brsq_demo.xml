<?xml version="1.0" encoding="utf-8"?>

<PropertyList>
  <scenario>
    <name>hrdb - Bourrasque's Wingmen</name>
    <description>
      3 wingmen - non-tactical
      This scenario formates 3 Bourrasque to the parent .ac at the position
      designated by the x,y,z offsets.
    </description>

    <entry n="0">
      <name>Bravo-Sierra</name>
      <type>wingman</type>
      <model>Aircraft/bourrasque/models/brsq.xml</model>
      <x-offset>-30</x-offset>
      <y-offset>30</y-offset>
      <z-offset>-10</z-offset>
      <formate>true</formate>
      <max-speed-kts>650</max-speed-kts>
      <coefficients>
        <heading>0.0</heading>
        <pitch>0.0</pitch>
        <bank>0.0</bank>
        <speed>1.4</speed>
      </coefficients>
    </entry>

    <entry n="1">
      <name>Bravo-Lima</name>
      <type>wingman</type>
      <model>Aircraft/bourrasque/models/brsq.xml</model>
      <x-offset>-30</x-offset>
      <y-offset>30</y-offset>
      <z-offset>-10</z-offset>
      <formate>true</formate>
      <max-speed-kts>650</max-speed-kts>
      <coefficients>
        <heading>0.0</heading>
        <pitch>0.0</pitch>
        <bank>0.0</bank>
        <speed>1.4</speed>
      </coefficients>
      <parent>Bravo-Sierra</parent>
    </entry>

    <entry n="2">
      <name>Bravo-Kilo</name>
      <type>wingman</type>
      <model>Aircraft/bourrasque/models/brsq.xml</model>
      <x-offset>-30</x-offset>
      <y-offset>30</y-offset>
      <z-offset>-10</z-offset>
      <formate>true</formate>
      <max-speed-kts>650</max-speed-kts>
      <coefficients>
        <heading>0.0</heading>
        <pitch>0.0</pitch>
        <bank>0.0</bank>
        <speed>1.4</speed>
      </coefficients>
      <parent>Bravo-Lima</parent>
    </entry>

  </scenario>

</PropertyList>
